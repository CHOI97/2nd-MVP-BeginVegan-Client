<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".src.ui.view.VeganTestBeforeFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <FrameLayout
            android:id="@+id/fl_vegan_test_description"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginHorizontal="24dp"
            android:layout_marginTop="156dp"/>
        <Button
            android:id="@+id/b_vegan_test_next"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="다음에 할게요"
            app:layout_constraintTop_toBottomOf="@id/fl_vegan_test_description"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/b_vegan_test_start"
            android:layout_marginTop="20dp"/>
        <Button
            android:id="@+id/b_vegan_test_start"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="테스트 시작하기"
            android:backgroundTint="@color/color_primary3"
            app:layout_constraintTop_toBottomOf="@id/fl_vegan_test_description"
            app:layout_constraintStart_toEndOf="@id/b_vegan_test_next"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="20dp"/>
        <ImageButton
            android:id="@+id/ib_vegan_test_share"
            android:layout_width="48dp"
            android:layout_height="48dp"
            app:layout_constraintTop_toBottomOf="@id/b_vegan_test_start"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/ic_test_share"
            android:background="@color/color_transparent" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>